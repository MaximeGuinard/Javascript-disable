{
    "manifest_version": 3,
    "name": "Disable JavaScript Toggle",
    "version": "1.0",
    "description": "Toggle JavaScript on the current page.",
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "contentSettings"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  